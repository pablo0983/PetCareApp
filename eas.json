{
  "cli": {
    "version": ">= 3.0.4",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false
      }
    },
    "production": {
      "env": {
        "EXPO_ANDROID_VERSION_CODE": "15"
      },
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "distribution": "store",
        "simulator": false
      }
    }
  },
  "submit": {
    "production": {}
  }
}
