{
  "name": "petcare-app",
  "version": "3.0.3",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/drawer": "^7.7.2",
    "@react-navigation/native": "^7.1.19",
    "@react-navigation/native-stack": "^7.6.2",
    "expo": "~54.0.0",
    "expo-camera": "~17.0.9",
    "expo-image-loader": "~6.0.0",
    "expo-image-picker": "~17.0.8",
    "expo-localization": "~17.0.7",
    "expo-location": "~19.0.7",
    "expo-notifications": "~0.32.12",
    "expo-print": "~15.0.7",
    "expo-sharing": "~14.0.7",
    "i18n-js": "^4.5.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-maps": "1.20.1",
    "react-native-modal-datetime-picker": "15.0.0",
    "react-native-nfc-manager": "^3.16.3",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-signature-canvas": "^5.0.1",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0"
  },
  "doctor": {
    "ignore": [
      "duplicateDependencies",
      "react-native-nfc-manager"
    ],
    "reactNativeDirectoryCheck": {
      "listUnknownPackages": false,
      "ignore": [
        "react-native-nfc-manager"
      ]
    }
  },
  "expo": {
    "name": "PetCareApp",
    "slug": "petcare-app",
    "version": "3.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/pata.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.pablopetry.petcareapp",
      "buildNumber": "9",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "Necesitamos acceso a la cámara para adjuntar imágenes en incidentes.",
        "NSPhotoLibraryAddUsageDescription": "Necesitamos permiso para guardar la firma como imagen en tu dispositivo.",
        "NSPhotoLibraryUsageDescription": "Necesitamos acceso a tu galería para adjuntar o guardar imágenes relacionadas con el incidente."
      }
    },
    "android": {
      "package": "com.pablopetry.PetCareApp",
      "versionCode": 13,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "daafa29d-29fd-4b32-8b99-be3cbe6833ad"
      }
    },
    "plugins": [],
    "owner": "pablopetry"
  },
  "resolutions": {
    "expo-image-loader": "6.0.0"
  }
}
